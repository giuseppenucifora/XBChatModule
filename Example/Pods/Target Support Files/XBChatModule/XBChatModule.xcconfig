CONFIGURATION_BUILD_DIR = $PODS_SHARED_BUILD_DIR/XBChatModule
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_SHARED_BUILD_DIR/JSQMessagesViewController" "$PODS_SHARED_BUILD_DIR/JSQSystemSoundPlayer" "$PODS_SHARED_BUILD_DIR/PNXMPPFramework" "$PODS_SHARED_BUILD_DIR/SDWebImage"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Public" $(SDKROOT)/usr/include/libxml2
OTHER_LDFLAGS = -l"xml2"
PODS_ROOT = ${SRCROOT}
PODS_SHARED_BUILD_DIR = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
